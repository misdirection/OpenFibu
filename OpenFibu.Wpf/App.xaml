<Application x:Class="OpenFibu.Wpf.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:OpenFibu.Wpf"
             xmlns:vk="clr-namespace:OpenFibu.Wpf.Vorkontierung"
             xmlns:stamm="clr-namespace:OpenFibu.Wpf.Stammdaten">
    <Application.Resources>
        <DataTemplate DataType="{x:Type vk:VorkontierungserfassungViewModel}">
            <vk:VorkontierungView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type stamm:SteuerschluesselViewModel}">
            <stamm:SteuerschluesselView/>
        </DataTemplate>
    </Application.Resources>
</Application>
